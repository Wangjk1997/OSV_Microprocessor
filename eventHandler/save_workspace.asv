function save_workspace(~,~)
    global pxHistory;
    global pyHistory;
    global yawHistory;
    global rawdataHistory;
    global fHistory;
    global duty_cyclesHistory;
    
    px_history 0 pxHistory;
    global pyHistory;
    global yawHistory;
    global rawdataHistory;
    global fHistory;
    global duty_cyclesHistory;
    tmp = clock;
    filename = ".\data\workspace" +num2str(tmp(2))+num2str(tmp(3))+num2str(tmp(4))+num2str(tmp(5));
    save(filename);
    disp('Control End' )
end